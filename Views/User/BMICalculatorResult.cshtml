

@{
    /**/

    ViewBag.Title = "BMICalulatorResult";
}
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<script>
    function calculate() {
        var weight = document.getElementById("txtWeight").value;
        var height = document.getElementById("txtHeight").value;
        var type = document.getElementById("ddlHeight").value;
        

        if (type == "CM") {
            height = height * 0.01;

            var bmi = weight / (height * height);
            document.getElementById("BMI").innerHTML = bmi ;
            if (bmi < 18.5) {
                document.getElementById("Status").innerHTML="Underweight";
            }
            else if (bmi > 18.5 && bmi <= 24.9) {
                document.getElementById("Status").innerHTML="Normal";
            }
            else if (bmi > 25.0 && bmi <= 29.9) {
                document.getElementById("Status").innerHTML = "Overweight";
            }
            else if (bmi > 30.0) {
               document.getElementById("Status").innerHTML="Obese";
            }
            else { return 0; }

            var btn = document.getElementById("button");
            btn.style.display = 'block';
            

        }
        else if (type == "Inches") {
            height = height * 0.0254;
            var bmi = weight / (height * height);
            document.getElementById("BMI").innerHTML = bmi;
            
            if (bmi < 18.5) {
                document.getElementById("Status").innerHTML="Underweight";
            }
            else if (bmi > 18.5 && bmi <= 24.9) {
                document.getElementById("Status").innerHTML="Normal";
            }
            else if (bmi > 25.0 && bmi <= 29.9) {
                document.getElementById("Status").innerHTML="Overweight";
            }
            else if (bmi > 30.0) {
               document.getElementById("Status").innerHTML="Obese";
            }
            else { return 0; }
            var btn = document.getElementById("button");
            btn.style.display = 'block';

        }
        else if (type == "Feet") {
            height = height / 3.281;
            var bmi = weight / (height * height);
            document.getElementById("BMI").innerHTML = bmi;
            
            if (bmi < 18.5) {
                document.getElementById("Status").innerHTML="Underweight";
            }
            else if (bmi > 18.5 && bmi <= 24.9) {
                document.getElementById("Status").innerHTML="Normal";
            }
            else if (bmi > 25.0 && bmi <= 29.9) {
                document.getElementById("Status").innerHTML = "Overweight";
            }
            else if (bmi > 30.0) {
               document.getElementById("Status").innerHTML="Obese";
            }
            else { return 0; }
            var btn = document.getElementById("button");
            btn.style.display = 'block';

        }

        else {
            alert("Please Fill in everything correctly");
        }




    }

</script>
<nav class="navbar navbar-expand-lg fixed-top navbar-dark bg-dark">
