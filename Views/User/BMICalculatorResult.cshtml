    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <a class="navbar-brand" href="@Url.Action("Home","BMI")" style="font-size:20px;color:white; margin-left:1450px;">
        <b>Logout</b><i class="fa fa-sign-out fa-4" style="font-size:20px;color:white; padding-left:5px;" aria-hidden="true"></i>
    </a>



</nav>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div>
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @if (ViewBag.Message != null)
        {
            <div class="form-group">
                <div>@ViewBag.Message</div>
            </div>
        }
        <div class="container-login100" style="margin-top:40px;">

            <div class="limiter">
                <div class="wrap-login100" style="width:800px;">
                    <div class="login100-form-title" style="background-image: url('data:image/jpeg;base64,');">
                        <span class="login100-form-title-1">
                            BMI Calculator
                        </span>
                    </div>

                    <form class="login100-form validate-form" name="bmiForm">
                        <div class="wrap-input100  m-b-26" style="margin-top:40px;margin-bottom:50px; margin-left:200px;">
                            <div class="form-group">
                                <span class="label-input100">Height Type</span>
                                <select name="ddlHeight" id="ddlHeight" class="form-control" required>

                                    <option value="CM">Centimeter(CM.)</option>
                                    <option value="Inches">Inches</option>
                                    <option value="Feet">Feet</option>
                                </select>

                            </div>
                        </div>
                        <div class="wrap-input100  m-b-26" style="margin-bottom:30px;margin-left:200px; border-bottom:none;">
                            <div class="form-group">

                                <span class="label-input100">Height</span>
                                <input name="txtHeight" type="text" id="txtHeight" class="form-control" required>
                            </div>
                            <span class="focus-input100"></span>
                        </div>
                        <div class="wrap-input100  m-b-26" style="margin-bottom:30px;margin-left:200px; border-bottom:none;">
                            <div class="form-group">
                                <span class="label-input100">Weight</span>
                                <input name="txtWeight" type="text" id="txtWeight" class="form-control" required>
                                
                            </div>
                            
                        </div>
                        <div class="wrap-input100  m-b-26" style="margin-bottom:30px;margin-left:200px; border-bottom:none;">
                            <div class="form-group">

                                <span class="label-input100">Age</span>
                                <input name="txtAge" type="number" id="txtAge" class="form-control" required>
                            </div>
                            
                            <span class="focus-input100"></span>
                        </div>
                        <div class="wrap-input100  m-b-26" style="margin-bottom:30px; margin-left:200px; border-bottom:none;">
                            <div class="form-group">
                                <span class="label-input100">Gender</span>

                                <div class="form-inline" style="color: black; margin-left:-15px;">

                                    <table id="rdbGender" class="form-control rbl" style="margin-top:4px; border:0;">
                                        <tbody>
                                            <tr>
                                                <td style="padding-right:100px;"><input id="rdbGender_0" type="radio" name="rdbGender" value="MALE" checked="checked">MALE</td>
                                                <td><input id="rdbGender_1" type="radio" name="rdbGender" value="FEMALE">FEMALE</td>
                                            </tr>
                                        </tbody>
                                    </table>

                                </div>

                                </div>
                            </div>

                                <div class="container-login100-form-btn">
                                    <input type="button" value="Calculate BMI" class="btn btn-lg btn-success pull-center" onclick="calculate()" style="margin-top:30px; margin-left:250px;">


                                </div>

                                
                                <p id="BMI" style="margin-top:20px; margin-left:300px;" ></p>
                                <p id="Status" style="margin-left:300px;"></p>
                                <input class="btn btn-success" id="button" style="border-style:none; display:none; margin-left:250px;" type="submit" value="ViewReport" />



                            
                        
                    </form>






                </div>
            </div>
        </div>
    </div>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
